mixin card({num, title, sUrl, sText, tFront, tBack, slug, img, imgType})
  a.card.bingo(data-slug=`${slug}` href=`/${tBack}/${slug}`)
    img(src=`https://s3.amazonaws.com/harrydry/gdmarketing/main/${img}.${imgType}`)
    h4=title
    .card__bottom
      .card__bottom-link
        .ideaSVG!=icon("ideaSVG")
        .pCard__bottom-link-text=sText
      .card__bottom-tag(class=`ct__${tBack}`)=tFront