    <mj-style inline="inline">
      .underA {
        color: #030212;
        text-decoration: none;
        border-bottom: 1px solid;
      }

      .underA {
        color: #030212;
        text-decoration: none;
        border-bottom: 1px solid;
      }

      .ss1 {
        border-bottom: 1px solid rgba(78, 204, 121, 1);
      }

      .ss2 {
        border-bottom: 1px solid rgba(10, 202, 255, 1);
      }

